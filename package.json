{
  "name": "obs-app",
  "version": "1.0.0",
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start": "expo start",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web",
    "build:android:debug": "cd android && node ../scripts/gradlew.js assembleDebug",
    "build:android:release": "cd android && node ../scripts/gradlew.js assembleRelease",
    "build:android:clean": "cd android && node ../scripts/gradlew.js clean",
    "install:android:debug": "adb install android/app/build/outputs/apk/debug/app-debug.apk",
    "install:android:release": "adb install android/app/build/outputs/apk/release/app-release.apk"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@types/jszip": "^3.4.1",
    "drizzle-kit": "^0.31.1",
    "drizzle-orm": "^0.44.0",
    "expo": "~52.0.46",
    "expo-asset": "^11.1.5",
    "expo-constants": "~17.0.8",
    "expo-document-picker": "~13.0.3",
    "expo-drizzle-studio-plugin": "^0.1.2",
    "expo-file-system": "~18.0.12",
    "expo-image-manipulator": "~13.0.6",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "~7.0.5",
    "expo-network": "~7.0.5",
    "expo-router": "~4.0.21",
    "expo-sdcard-detector": "^1.0.0",
    "expo-sharing": "~13.0.1",
    "expo-splash-screen": "~0.29.24",
    "expo-sqlite": "~15.1.4",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.9",
    "inversify": "^7.5.1",
    "jszip": "^3.10.1",
    "jszip-utils": "^0.1.0",
    "nativewind": "latest",
    "react": "18.3.1",
    "react-native": "0.76.9",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "3.16.2",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.3.12",
    "@types/react-native": "^0.73.0",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^12.0.1",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.3.3"
  },
  "eslintConfig": {
    "extends": "universe/native",
    "root": true
  },
  "main": "expo-router/entry",
  "private": true
}
